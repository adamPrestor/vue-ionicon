{
  "name": "vue-ionicon",
  "version": "1.0.0",
  "description": "Caching asynchronous IonIcons loader for Vue",
  "author": "Jure Erzno≈ænik",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "ion-icon.vue",
  "files": [
    "ion-icon.vue"
  ],
  "keywords": [
    "vue ionicons",
    "vue svg",
    "ionicons"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:velis74/vue-ionicon.git"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "dompurify": "^2.3.4",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "cache-loader": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-vue": "^8.3.0",
    "flush-promises": "^1.0.2",
    "np": "^7.6.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "vue": {
    "outputDir": "dist/",
    "css": {
      "extract": {
        "ignoreOrder": true,
        "filename": "css/vue-ionicon.css",
        "chunkFilename": "css/vue-ionicon-vendor.css"
      }
    },
    "configureWebpack": {
      "entry": {
        "app": "./index.js"
      },
      "resolve": {
        "extensions": [
          ".js",
          ".vue",
          ".json"
        ]
      },
      "devServer": {
        "proxy": {
          ".*": {
            "target": "http://localhost:8000",
            "secure": false
          }
        }
      },
      "output": {
        "filename": "js/vue-ionicon.js",
        "chunkFilename": "js/vue-ionicon-vendors.js"
      }
    }
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "plugins": [
      "es"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "no-console": 0,
      "no-plusplus": "off",
      "sort-imports": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ],
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 1
        }
      ],
      "es/no-destructuring": "error",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "prefer-destructuring": [
        "warn",
        {
          "VariableDeclarator": {
            "array": false,
            "object": false
          },
          "AssignmentExpression": {
            "array": false,
            "object": false
          }
        },
        {
          "enforceForRenamedProperties": false
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "operator-linebreak": [
        "error",
        "after"
      ],
      "linebreak-style": [
        "off"
      ],
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "never"
        }
      ],
      "vue/no-v-html": [
        "off"
      ],
      "vue/max-attributes-per-line": [
        "warn",
        {
          "singleline": {
            "max": 10
          }
        }
      ],
      "vue/singleline-html-element-content-newline": [
        "off"
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "testMatch": [
      "**/*.spec.[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!index.*"
    ]
  }
}
